{
	"name": "Discord Theme (Dark)",
	"type": "dark",
	"colors": {
		"editor.background": "#36393f",
		"editor.foreground": "#ffffff",
		"activityBarBadge.background": "#F04747",
		"sideBarTitle.foreground": "#ffffff",
		"sideBarSectionHeader.background": "#2f3136",
		"statusBar.background": "#202225",
		"sideBar.background": "#2f3136",
		"list.hoverBackground": "#2f3136",
		"titleBar.activeBackground": "#202225",
		"titleBar.inactiveBackground": "#202225",
		"activityBar.background": "#202225",
		"menubar.selectionBackground": "#4F545C",
		"menu.background": "#202225",
		"editorGroupHeader.tabsBackground": "#2f3136",
		"tab.activeBackground": "#36393f",
		"tab.inactiveBackground": "#2f3136",
	},

	"tokenColors": [
        // Command Palette : "Developer: Inspect Editor Tokens and Scopes"

    // Python
        {
            "scope": "variable.language.special.self.python",
			"settings": {
				"foreground": "#38c0ff"
			}, 
		},
        {
            "scope": [
                "meta.function-call.python",
                "meta.function-call.generic.python"
            ],
            "settings": {
                "foreground": "#8ddbff"
            }
        },
        {
            "scope": [
                "storage.type.function.python",
                "meta.function.python"
            ],
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": [
                "storage.type.class.python",
                "meta.class.python"
            ],
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": "punctuation.separator.period.python",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "source.python",
            "settings": {
                "foreground": "#8ddbff"
            }
        },
        {
            "scope": "variable.parameter.function-call.python",
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": "storage.type.string.python",
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": [
                "punctuation.definition.arguments.begin.python",
                "punctuation.definition.arguments.end.python"
            ],
            "settings": {
                "foreground": "#ffe449"
            }
        },

    // Javascript
        {
            "scope": [
                "storage.type.js",
                "meta.var.expr.js"
            ],
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": [
                "storage.type.class.js",
                "meta.class.js"
            ],
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": [
                "storage.type.function.js",
                "meta.function.js"
            ],
            "settings": {
                "foreground": "#38c0ff"
            }
        },
        {
            "scope": "punctuation.definition.block.js",
            "settings": {
                "foreground": "#8DDBFF"
            }
        },
        {
            "scope": "meta.brace.round.js",
            "settings": {
                "foreground": "#fff9da"
            }
        },
        {
            "scope": "meta.arrow",
            "settings": {
                "foreground": "#cc72c5"
            }
        },
        {
            "scope": "punctuation.terminator.statement.js",
            "settings": {
                "foreground": "#ffe449"
            }
        },
        {
            "scope": "support.type.object.module.js",
            "settings": {
                "foreground": "#cc72c5"
            }
        },
        
    // Json
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#8ddbff"
            }
        },

    // Others
        {
            // Class name
            "scope": [
                "support.class",
                "entity.name.class",
                "entity.other.attribute-name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#8dff9c"
            }
        },
        {
            // Class arguments
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#b4ffbe"
            }
        },
        {
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#cc72c5"
			}
		},
        {
            "scope": [
                "keyword.control",
                "keyword.operator.delete",
                "keyword.other.using",
                "keyword.other.operator",
                "entity.name.operator"
            ],
            "settings": {
                "foreground": "#cc72c5"
            }
        },
        {
			"scope": "comment",
			"settings": {
				"foreground": "#6ab646"
			}
		},
        {
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#d3ffbb"
			}
		},
        {
			"scope": "invalid",
			"settings": {
				"foreground": "#F44747"
			}
		},
        
        {
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable",
                "variable.other.constant",
				"variable.other.enummember",
                "constant.language",
			],
			"settings": {
				"foreground": "#8ddbff"
			}, 
		},
        {
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#ffe449"
			}
		},
        {
			"scope": [
				"string",
                "string.value",
                "string.tag"
			],
			"settings": {
				"foreground": "#ff837e"
			}
		},
        {
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal"
			],
			"settings": {
				"foreground": "#ffe18f"
			}
		},
	]
}