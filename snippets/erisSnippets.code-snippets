{
	"Default Discord bot index.js file": {
		"prefix": ["eris.index", "eris.main", "eris.bot", "eris.client"],
		"body": [
			"const Eris = require(\"eris\");",
            "",
            "const bot = new Eris.CommandClient(\"${1:TOKEN}\", {}, {",
            "\tdescription: \"A template bot made with Eris\",",
            "\towner: \"${2:OWNER}\",",
            "\tprefix: \"${3:!}\"",
            "});",
            "",
            "bot.on(\"ready\", () => { // When the bot is ready",
            "\tconsole.log(\"Ready!\"); // Log \"Ready!\"",
            "});",
            "",
            "bot.registerCommand(\"echo\", (msg, args) => { // Make an echo command",
            "\tif(args.length === 0) { // If the user just typed \"!echo\", say \"Invalid input\"",
            "\t\treturn \"Invalid input\";",
            "\t}",
            "\tconst text = args.join(\" \"); // Make a string of the text after the command label",
            "\treturn text; // Return the generated string",
            "}, {",
            "\tdescription: \"Make the bot say something\",",
            "\tfullDescription: \"The bot will echo whatever is after the command label.\",",
            "\tusage: \"<text>\"",
            "});",
            "",
            "bot.connect();"
		],
        "description": "Create a basic Discord bot index.js file."
    },

    "Default Discord bot command": {
		"prefix": ["eris.cmd", "eris.command"],
		"body": [       
            "bot.registerCommand(\"${1:commandName}\", (msg, args) => {",
            "    return \"pong!\";",
            "}, {",
            "    description: \"${2:description}\",",
            "    fullDescription: \"${3:fullDescription}\",",
            "    usage: \"${4:usage}\"",
            "});"
		],
        "description": "Create a basic Discord command."
    },

    "Complex Discord bot command": {
		"prefix": ["eris.cmd+", "eris.command+"],
		"body": [       
            "bot.registerCommand(\"${1:commandName}\", (msg, args) => {",
            "    return \"pong!\";",
            "}, {",
            "    description: \"${2:description}\",",
            "    fullDescription: \"${3:fullDescription}\",",
            "    usage: \"${4:usage}\"",
            "});",
            "",
            "bot.registerCommandAlias(\"${5:aliase}\", \"${1:commandName}\");"
		],
        "description": "Create a complex Discord command."
    },

    "Default Discord bot embed": {
		"prefix": "eris.embed",
		"body": [       
            "bot.createMessage(msg.channel.id, {",
            "\tembed: {",
            "\t\ttitle: \"${1:title}\",",
            "\t\tdescription: \"${2:description}\",",
            "\t\t\tcolor: ${3:#ffffff}, // Color, either in hex (show), or a base-10 integer",
            "\t\t\tfields: [ // Array of field objects",
            "\t\t\t\t{",
            "\t\t\t\t\tname: \"${5:name}\", // Field title",
            "\t\t\t\t\tvalue: \"${6:value}\", // Field",
            "\t\t\t\t\tinline: true // Whether you want multiple fields in same line",
            "\t\t\t},",
            "\t\t]",
            "\t}",
            "});",
		],
        "description": "Create a basic Discord embed."
    },

    "Complex Discord bot embed": {
		"prefix": "eris.embed+",
		"body": [       
            "bot.createMessage(msg.channel.id, {",
            "\tembed: {",
            "\t\ttitle: \"${1:title}\",",
            "\t\tdescription: \"${2:description}\",",
            "\t\t\tauthor: { // Author property",
            "\t\t\tname: msg.author.username,",
            "\t\t\t\ticon_url: msg.author.avatarURL",
            "\t\t},",
            "\t\t\tcolor: ${3:#ffffff}, // Color, either in hex (show), or a base-10 integer",
            "\t\t\tfields: [ // Array of field objects",
            "\t\t\t\t{",
            "\t\t\t\t\tname: \"${5:name}\", // Field title",
            "\t\t\t\t\tvalue: \"${6:value}\", // Field",
            "\t\t\t\t\tinline: true // Whether you want multiple fields in same line",
            "\t\t\t},",
            "\t\t],",
            "\t\t\tfooter: { // Footer text",
            "\t\t\ttext: \"{7:text}\"",
            "\t\t}",
            "\t}",
            "});",
		],
        "description": "Create a complex Discord embed."
    },

    "Default Discord bot message": {
		"prefix": "eris.message",
