            "bot.on(\"messageCreate\", (msg) => {",
            "\tif(msg.content === \"!ping\") {",
            "\t\tbot.createMessage(msg.channel.id, \"{1:pong!}\");",
            "\t}",
            "});",
		],
        "description": "Create a default Discord bot message."
